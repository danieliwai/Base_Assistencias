CREATE TABLE `base_gi`.`temp_ac_assistencia` (
`TIPO_ATENDIMENTO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`CLIENTE_INSTITUCIONAL` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`COD_PRODUTO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`DESC_PRODUTO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`NUM_ASSISTENCIA` VARCHAR(45) CHARACTER SET latin1 NOT NULL
,`DATA_DE_CRIACAO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`ABSENT` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`COD_CAUSA` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`DESC_CAUSA` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`COD_PANE` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`DESC_PANE` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`COD_STATUS` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`APOLICE` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`NOME_CLIENTE` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`UF` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`CIDADE` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`DOCUMENTO_CLIENTE` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`PAIS` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`UF_ASSISTENCIA` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`CIDADE_ASSISTENCIA` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`MARCA` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`MODELO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`CHASSI` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`PLACA` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`ANO_VEICULO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`CONTRATO_PLANO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`SGC_PLANO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`INICIO_VIGENCIA` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`FIM_VIGENCIA` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`NOME_CORRETOR` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`EMISSION_ORIGIN` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`POLIZA_AZ` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`APLICA_AZ` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1 COLLATE=latin1_general_ci
