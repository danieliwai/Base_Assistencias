CREATE TABLE `ac_servico` (
  `NUM_ASSISTENCIA` INT(10) DEFAULT NULL
  ,`DATA_DE_CRIACAO_SOLICITACAO` DATETIME DEFAULT NULL
  ,`NUM_SOLICITACAO` INT(3) DEFAULT NULL
  ,`COD_SERVICO` VARCHAR(10) DEFAULT NULL
  ,`DESC_SERVICO` VARCHAR(45) DEFAULT NULL
  ,`COD_ESPECIALIDADE` VARCHAR(10) DEFAULT NULL
  ,`DESC_ESPECIALIDADE` VARCHAR(45) DEFAULT NULL
  ,`ESTADO_SOLICITACAO` VARCHAR(5) DEFAULT NULL
  ,`CIDADE_SOLICITACAO` VARCHAR(45) DEFAULT NULL
  ,`ENDERECO_SOLICITACAO` VARCHAR(45) DEFAULT NULL
  ,`BAIRRO_SOLICITACAO` VARCHAR(45) DEFAULT NULL
  ,`CEP_DA_SOLICITACAO` VARCHAR(20) DEFAULT NULL
  ,`AGENDADO` VARCHAR(10) DEFAULT NULL
  ,`COD_PRESTADOR` VARCHAR(20) DEFAULT NULL
  ,`NOME_PRESTADOR` VARCHAR(45) DEFAULT NULL
  ,`STATUS_SOLICITACAO` VARCHAR(20) DEFAULT NULL
  ,`TIPO_CARRO_RESERVA` VARCHAR(45) DEFAULT NULL
  ,`DIARIAS` INT(6) DEFAULT NULL
  ,`PROBLEMA` VARCHAR(45) DEFAULT NULL
  ,`COD_OFICINA_DESTINO` VARCHAR(15) DEFAULT NULL
  ,`DATA_DE_CRIACAO_OP` DATETIME DEFAULT NULL
  ,`NUMERO_DA_OP` INT(15) NOT NULL
  ,`STATUS` VARCHAR(20) DEFAULT NULL
  ,`CUSTO_COMBINADO_OP` INT(20) DEFAULT NULL,
  PRIMARY KEY (`NUMERO_DA_OP`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
