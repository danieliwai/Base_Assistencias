CREATE TABLE `ac_tarifa` (
  `NUM_ASSISTENCIA` INT(10) DEFAULT NULL
  ,`NUM_SOLICITACAO` INT(3) DEFAULT NULL
  ,`NUMERO_DA_OP` INT(15) DEFAULT NULL
  ,`ID_DO_ITEM` INT(15) NOT NULL
  ,`TIPO_DE_TARIFA` VARCHAR(45) DEFAULT NULL
  ,`DATA_DE_CRIACAO_TARIFA` DATETIME DEFAULT NULL
  ,`STATUS` VARCHAR(20) DEFAULT NULL
  ,`QUANTIDADE` INT(20) DEFAULT NULL
  ,`VALOR_UNITARIO` INT(20) DEFAULT NULL
  ,`VALOR_PAGO` INT(20) DEFAULT NULL,
  PRIMARY KEY (`ID_DO_ITEM`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
