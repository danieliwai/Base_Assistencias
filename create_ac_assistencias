CREATE TABLE `ac_assistencias` (
  `TIPO_ATENDIMENTO` VARCHAR(40) DEFAULT NULL
  ,`CLIENTE_INSTITUCIONAL` VARCHAR(40) DEFAULT NULL
  ,`COD_PRODUTO` VARCHAR(20) DEFAULT NULL
  ,`DESC_PRODUTO` VARCHAR(40) DEFAULT NULL
  ,`NUM_ASSISTENCIA` INT(11) NOT NULL
  ,`DATA_DE_CRIACAO` DATETIME DEFAULT NULL
  ,`ABSENT` VARCHAR(10) DEFAULT NULL
  ,`COD_CAUSA` VARCHAR(10) DEFAULT NULL
  ,`DESC_CAUSA` VARCHAR(40) DEFAULT NULL
  ,`COD_PANE` VARCHAR(10) DEFAULT NULL
  ,`DESC_PANE` VARCHAR(40) DEFAULT NULL
  ,`COD_STATUS` VARCHAR(10) DEFAULT NULL
  ,`APOLICE` VARCHAR(20) DEFAULT NULL
  ,`NOME_CLIENTE` VARCHAR(45) DEFAULT NULL
  ,`UF` VARCHAR(5) DEFAULT NULL
  ,`CIDADE` VARCHAR(45) DEFAULT NULL
  ,`DOCUMENTO_CLIENTE` VARCHAR(20) DEFAULT NULL
  ,`PAIS` VARCHAR(40) DEFAULT NULL
  ,`UF_ASSISTENCIA` VARCHAR(5) DEFAULT NULL
  ,`CIDADE_ASSISTENCIA` VARCHAR(45) DEFAULT NULL
  ,`MARCA` VARCHAR(20) DEFAULT NULL
  ,`MODELO` VARCHAR(40) DEFAULT NULL
  ,`CHASS1` VARCHAR(20) DEFAULT NULL
  ,`PLACA` VARCHAR(15) DEFAULT NULL
  ,`ANO_VEICULO` VARCHAR(10) DEFAULT NULL
  ,`CONTRATO_PLANO` VARCHAR(40) DEFAULT NULL
  ,`SGC_PLANO` VARCHAR(40) DEFAULT NULL
  ,`INICIO_VIGENCIA` DATE DEFAULT NULL
  ,`FIM_VIGENCIA` DATE DEFAULT NULL
  ,`EMISSION_ORIGIN` VARCHAR(40) DEFAULT NULL
  ,`POLIZA_AZ` VARCHAR(40) DEFAULT NULL
  ,`APLICA_AZ` VARCHAR(40) DEFAULT NULL
  
  PRIMARY KEY (`NUM_ASSISTENCIA`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
