CREATE TABLE `base_gi`.`temp_ac_servico` (
`NUM_ASSISTENCIA` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`DATA_DE_CRIACAO_SOLICITACAO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`NUM_SOLICITACAO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`COD_SERVICO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`DESC_SERVICO` VARCHAR(45) CHARACTER SET latin1 DEFAULT NULL
,`COD_ESPECIALIDADE` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`DESC_ESPECIALIDADE` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`ESTADO_SOLICITACAO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`CIDADE_SOLICITACAO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`ENDERECO_SOLICITACAO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`BAIRRO_SOLICITACAO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`CEP_DA_SOLICITACAO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`AGENDADO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`NOME_PRESTADOR` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`COD_PRESTADOR` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`STATUS_SOLICITACAO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`TIPO_CARRO_RESERVA` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`DIARIAS` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`PROBLEMA` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`COD_OFICINA_DESTINO` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`DATA_DE_CRIACAO_OP` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`NUMERO_DA_OP` VARCHAR(100) COLLATE latin1_general_ci NOT NULL
,`STATUS` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
,`CUSTO_COMBINADO_OP` VARCHAR(100) COLLATE latin1_general_ci DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1 COLLATE=latin1_general_ci;
